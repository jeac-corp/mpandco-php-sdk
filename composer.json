{
    "name": "jeac-corp/mpandco-php-sdk",
    "description": "Comunicacion con la api de mPandco",
    "keywords": ["mpandco", "payments", "rest", "sdk"],
    "type": "library",
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Carlos Mendoza",
            "email": "inhack20@gmail.com"
        }
    ],
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/options-resolver": "~3.4 | ~4.2",
        "jms/serializer": "~1.12.1",
        "league/oauth2-client": "2.3.0",
        "monolog/monolog": "~1.23"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "v4.1.1"
    },
    "autoload": {
        "psr-4": { "JeacCorp\\": "lib/" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
